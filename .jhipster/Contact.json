    },
    {
      "relationshipType": "one-to-many",
      "otherEntityName": "contactGroup",
      "otherEntityRelationshipName": "contact",
      "relationshipName": "groups"
    },
    {
      "relationshipType": "one-to-many",
      "otherEntityName": "customField",
      "otherEntityRelationshipName": "contact",
      "relationshipName": "customFields"
    }
  ],
  "javadoc": "Contact",
  "entityTableName": "contact",
