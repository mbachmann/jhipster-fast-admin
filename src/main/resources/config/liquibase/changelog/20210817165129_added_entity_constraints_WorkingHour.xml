    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity WorkingHour.
    -->
    <changeSet id="20210817165129-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="application_user_id"
                                 baseTableName="working_hour"
                                 constraintName="fk_working_hour__application_user_id"
